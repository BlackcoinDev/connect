[include]
.*/node_modules/trezor-link

[ignore]
.*/build/.*
.*/trezor-common/.*
.*/ts/.*
.*/plugins/.*
.*/npm/.*
.*/npm-extended/.*


[libs]
./src/flowtype/

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
esproposal.decorators=ignore
module.name_mapper='.*\(.less\)' -> 'CSSModule'
module.system=haste
